{
  "name": "codenames",
  "version": "1.0.0",
  "description": "Online version of the popular boardgame Codenames",
  "main": "index.js",
  "scripts": {
    "start": "nodemon server/server.js",
    "test": "node scripts/test.js --env=jsdom"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@gfpacheco/redux-form-material-ui": "^1.0.0-beta.4",
    "axios": "^0.15.2",
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-class-properties": "^6.16.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.16.3",
    "body-parser": "^1.15.2",
    "classnames": "^2.2.5",
    "cors": "^2.8.1",
    "express": "^4.14.0",
    "flexboxgrid": "^6.3.1",
    "knex": "^0.12.6",
    "material-ui": "^1.0.0-beta.16",
    "morgan": "^1.7.0",
    "nodemon": "^1.12.1",
    "pg": "^6.1.0",
    "prop-types": "^15.6.0",
    "radium": "^0.18.1",
    "react": "^16.0.0-rc.3",
    "react-dom": "^16.0.0-rc.3",
    "react-flexbox-grid": "^0.10.2",
    "react-redux": "^4.4.6",
    "react-router": "^3.0.0",
    "react-transition-group": "^2.2.1",
    "redux": "^3.6.0",
    "redux-form": "^6.2.0",
    "redux-logger": "^2.7.4",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.1.0",
    "reselect": "^2.5.4",
    "socket.io": "^1.5.1",
    "socket.io-client": "^1.5.1",
    "webpack": "^1.13.2",
    "webpack-dev-middleware": "^1.8.4"
  },
  "devDependencies": {
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-env": "^1.6.0",
    "css-loader": "^0.25.0",
    "dotenv": "^4.0.0",
    "enzyme": "^3.1.0",
    "jest": "^21.2.1",
    "jest-bamboo-formatter": "^1.0.1",
    "json-loader": "^0.5.4",
    "moxios": "^0.4.0",
    "npm": "^4.0.2",
    "object-assign": "^4.1.1",
    "promise": "^8.0.1",
    "style-loader": "^0.13.1"
  },
  "jest": {
    "testResultsProcessor": "./node_modules/jest-bamboo-formatter",
    "cacheDirectory": "./jest",
    "clearMocks": true,
    "collectCoverageFrom": [
      "client/**/*.{js,jsx}",
      "server/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "roots": [
      "client",
      "server"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](build|docs|node_modules|scripts)[/\\\\]"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    }
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
